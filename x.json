{
  "name": "http",
  "description": "HTTP nodes for ChiÏ‡",
  "version": "0.0.1",
  "licences": [
    {
      "type": "MIT",
      "url": "https://github.com/psichi/chix/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/psichi/chix-http.git"
  },
  "dependencies": {},
  "nodes": [
    {
      "ports": {
        "input": {
          "res": {
            "type": "object"
          }
        },
        "output": {}
      },
      "description": "http onData result",
      "name": "onData",
      "ns": "http",
      "fn": "res.body += chunk\n"
    },
    {
      "ports": {
        "input": {
          "url": {
            "type": "uri"
          }
        },
        "output": {
          "res": {
            "type": "object"
          }
        }
      },
      "require": {
        "fs": "*"
      },
      "description": "http get",
      "name": "get",
      "ns": "http",
      "fn": "output = [http, 'get', input.url]\n"
    },
    {
      "ports": {
        "input": {
          "res": {
            "type": "object"
          }
        },
        "output": {
          "chunk": {
            "type": "object"
          }
        }
      },
      "description": "http onData result",
      "name": "onData",
      "ns": "http",
      "fn": "output = [res, 'on', 'data']\n"
    }
  ],
  "twigs": [
    {
      "name": "get",
      "description": "HTTP get",
      "nodes": [
        {
          "ns": "http",
          "name": "get",
          "context": {
            "url": "https://github.com/psichi/chix-website/raw/master/website.md"
          }
        },
        {
          "ns": "http",
          "name": "onData"
        },
        {
          "ns": "http",
          "name": "body"
        },
        {
          "ns": "express",
          "name": "send"
        }
      ],
      "links": [
        {
          "source": 0,
          "target": 1,
          "out": "res",
          "in": "res"
        },
        {
          "source": 1,
          "target": 2,
          "out": "res",
          "in": "chunk"
        },
        {
          "source": 1,
          "target": 3,
          "out": "res",
          "in": "res"
        }
      ],
      "ns": "http"
    }
  ]
}