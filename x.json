{
  "name": "http",
  "description": "HTTP nodes for ChiÏ‡",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/http.git"
  },
  "dependencies": {
    "request": "2.9.x"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "url": {
            "type": "string",
            "format": "uri",
            "title": "Url Request",
            "description": "The url to be requested.",
            "required": true
          }
        },
        "output": {
          "error": {
            "type": "object"
          },
          "res": {
            "type": "object"
          },
          "body": {
            "type": "string"
          }
        }
      },
      "require": {
        "request": "2.9.x"
      },
      "phrases": {
        "active": "Performing HTTP request to {{input.url}}"
      },
      "description": "http request",
      "ns": "http",
      "name": "request",
      "fn": "var obj = {};\nobj.request = request;\noutput = [obj, 'request', input.url]\n"
    }
  ],
  "twigs": []
}