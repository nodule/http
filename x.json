{
  "name": "http",
  "description": "HTTP nodes for ChiÏ‡",
  "version": "0.0.1",
  "licences": [
    {
      "type": "MIT",
      "url": "https://github.com/psichi/chix/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/psichi/chix-http.git"
  },
  "dependencies": {},
  "nodes": [
    {
      "ports": {
        "input": {
          "url": {
            "type": "string",
            "format": "uri"
          }
        },
        "output": {
          "error": {
            "type": "object"
          },
          "res": {
            "type": "object"
          },
          "body": {
            "type": "string"
          }
        }
      },
      "require": {
        "request": "latest"
      },
      "description": "http request",
      "name": "request",
      "ns": "http",
      "fn": "var obj = {};\nobj.request = request;\noutput = [obj, 'request', input.url]"
    }
  ],
  "twigs": [
    {
      "name": "get",
      "description": "HTTP get",
      "nodes": [
        {
          "ns": "http",
          "name": "get",
          "context": {
            "url": "https://github.com/psichi/chix-website/raw/master/website.md"
          }
        },
        {
          "ns": "http",
          "name": "onData"
        },
        {
          "ns": "http",
          "name": "body"
        },
        {
          "ns": "express",
          "name": "send"
        }
      ],
      "links": [
        {
          "source": 0,
          "target": 1,
          "out": "res",
          "in": "res"
        },
        {
          "source": 1,
          "target": 2,
          "out": "res",
          "in": "chunk"
        },
        {
          "source": 1,
          "target": 3,
          "out": "res",
          "in": "res"
        }
      ],
      "ns": "http"
    }
  ]
}