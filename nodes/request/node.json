{
  "name":"request",
  "ns": "http",
  "description":"http request",
  "phrases": {
    "active": "Performing HTTP request to {{input.url}}"
  },
  "require":{
    "request":"latest"
  },
  "ports":{
    "input":{
      "url":{
        "type":"string",
        "format":"uri",
        "title":"Url Request",
        "description":"The url to be requested.",
        "required": true
      }
    },
    "output":{
      "error":{
        "type":"object"
      },
      "res":{
        "type":"object"
      },
      "body":{
        "type":"string"
      }
    }
  }
}
